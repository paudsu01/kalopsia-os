{
    "llvm-target": "x86_64-unknown-none",
    "data-layout": "e-m:e-p270:32:32-p271:32:32-p272:64:64-i64:64-i128:128-f80:128-n8:16:32:64-S128",
    "arch": "x86_64",
    "target-endian": "little",
    "target-pointer-width": 64,
    "target-c-int-width": 32,
    "os": "none",
    "executables": true,
    "linker-flavor": "ld.lld",
    "linker": "rust-lld",
    
    "_comment_1": "// Disable stack unwinding on panic",
    "panic-strategy": "abort",

    "_comment_2": "// Disable redzone optimization because the hardware can overwrite the data in the red zone during interrupts",
    "disable-redzone": true,

    "_comment_3": "// Emulate floating point operations because SIMD registers are large -> perfomance problems",
    "features": "-mmx,-sse,+soft-float",
    "rustc-abi": "x86-softfloat"
}
